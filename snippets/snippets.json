  "tent-form:example": {
    "prefix": "tent-form:example",
    "body": "<form>\r\n<div class=\"grid\">\r\n<div class=\"grid__column\">\r\n<div class=\"control control--block\">\r\n<label class=\"control__label\" for=\"exampleEmailInput\">Your email<\/label>\r\n<input class=\"control__input\" type=\"email\" placeholder=\"test@domain.com\" id=\"exampleEmailInput\">\r\n<\/div>\r\n<\/div>\r\n<div class=\"grid__column\">\r\n<label class=\"control__label\" for=\"exampleSelect\">What can we help you with?<\/label>\r\n<div class=\"control control--block control--select\">\r\n<select class=\"control__select\" id=\"exampleSelect\">\r\n<option value=\"Option 1\">Questions<\/option>\r\n<option value=\"Option 2\">Feedback<\/option>\r\n<\/select>\r\n<\/div>\r\n<\/div>\r\n<\/div>\r\n<div class=\"grid\">\r\n<div class=\"grid__column\">\r\n<div class=\"control control--block\">\r\n<label class=\"control__label\" for=\"exampleMessage\">Message<\/label>\r\n<textarea class=\"control__textarea control__textarea--md\" placeholder=\"Type your message here...\" id=\"exampleMessage\"><\/textarea>\r\n<\/div>\r\n<\/div>\r\n<\/div>\r\n<div class=\"grid\">\r\n<div class=\"grid__column\">\r\n\r\n<div class=\"control control--checkbox\">\r\n<label class=\"control__label\" class=\"checkbox\">\r\n<input class=\"control__checkbox\" type=\"checkbox\">\r\nA checkbox\r\n<\/label>\r\n<\/div>\r\n<div class=\"control control--radio\">\r\n<label class=\"control__label\">\r\n<input class=\"control__radio\" type=\"radio\" name=\"question\">\r\nOne radio example\r\n<\/label>\r\n<label class=\"control__label\">\r\n<input class=\"control__radio\" type=\"radio\" name=\"question\">\r\nSecond radio example\r\n<\/label>\r\n<\/div>\r\n\r\n<\/div>\r\n<\/div>\r\n<div class=\"grid\">\r\n<div class=\"grid__column\">\r\n<div class=\"control control--block\">\r\n<input class=\"control__button button button--filled button--primary\" type=\"submit\" value=\"Submit\">\r\n<\/div>\r\n<\/div>\r\n<\/div>\r\n<\/form>",
    "description": "Form Example",
    "scope": ""
  },
  "tent-form:control-set": {
